<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example2.parth.sample_firebase_app">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.NFC" />

    <application
        android:name=".Sample_Firebase_App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AfterLoginSupremeUsers"
            android:label="@string/title_activity_after_login_supreme_users"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LoginPatient"
            android:label="@string/title_activity_login_patient"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ReadTagActivity"
            android:label="@string/title_activity_read_tag"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SignUpPatientActivity"
            android:label="@string/title_activity_sign_up_patient"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
